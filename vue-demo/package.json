		"@tsconfig/node18": "^18.2.0",
		"@types/node": "^18.17.5",
		"@vitejs/plugin-vue": "^4.3.1",
		"@vue/tsconfig": "^0.4.0",
		"prettier": "^2.7.1",
		"npm-run-all": "^4.1.5",
		"typescript": "~5.1.6",
		"vite": "^4.4.9",
		"vue-tsc": "^1.8.8"
