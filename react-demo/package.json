{
	"name": "@fluid-example/react-demo",
	"version": "0.1.0",
	"private": true,
	"description": "An example app built using Create React App plus a custom Fluid React hook",
	"homepage": "https://fluidframework.com",
	"repository": "microsoft/FluidExamples",
	"license": "MIT",
	"author": "Microsoft and contributors",
	"sideEffects": false,
	"main": "dist/index.js",
	"module": "lib/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "react-scripts build",
		"ci:test": "start-server-and-test start:server 7070 ci:test:jest",
		"ci:test:jest": "jest --ci --reporters=default --reporters=jest-junit",
		"eject": "react-scripts eject",
		"format": "npm run prettier:fix",
		"lint": "npm run prettier",
		"lint:fix": "npm run prettier:fix",
		"prettier": "prettier --check . --ignore-path ./.prettierignore",
		"prettier:fix": "prettier --write . --ignore-path ./.prettierignore",
		"start": "react-scripts start",
		"start:server": "npx tinylicious",
		"test": "start-server-and-test start:server 7070 test:jest",
		"test:jest": "jest"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"engines": {
		"node": ">=18.17.0"
	},
	"dependencies": {
		"@fluidframework/tinylicious-client": "^1.3.7",
		"fluid-framework": "^1.3.7",
		"react": "^16.10.2",
		"react-dom": "^16.10.2"
	},
	"devDependencies": {
		"@fluidframework/build-common": "^2.0.0",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"jest": "^29.2.2",
		"jest-junit": "^16.0.0",
		"jest-puppeteer": "^9.0.0",
		"prettier": "^2.7.1",
		"puppeteer": "^21.0.0",
		"react-scripts": "^5.0.1",
		"start-server-and-test": "^2.0.0",
		"tinylicious": "^1.0.0"
	},
	"jest-junit": {
		"outputDirectory": "nyc",
		"outputName": "jest-junit-report.xml"
	}
}
