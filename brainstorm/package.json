{
	"name": "@fluid-example/brainstorm",
	"version": "0.1.0",
	"private": true,
	"description": "A simple brainstorming app built using Create React App plus a Fluid data model",
	"homepage": "https://fluidframework.com",
	"repository": "microsoft/FluidExamples",
	"license": "MIT",
	"author": "Microsoft and contributors",
	"sideEffects": false,
	"main": "dist/index.js",
	"module": "lib/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "react-scripts build",
		"ci:test": "start-server-and-test start:server 7070 ci:test:jest",
		"ci:test:jest": "jest --ci --reporters=default --reporters=jest-junit",
		"eject": "react-scripts eject",
		"start": "react-scripts start",
		"start:server": "npx @fluidframework/azure-local-service",
		"start:azure": "cross-env REACT_APP_FLUID_CLIENT='\"azure\"' npm run start",
		"test": "start-server-and-test start:server 7070 test:jest",
		"test:jest": "jest",
		"format": "npm run prettier:fix",
		"lint": "npm run prettier",
		"lint:fix": "npm run prettier:fix",
		"prettier": "prettier --check . --ignore-path ./.prettierignore",
		"prettier:fix": "prettier --write . --ignore-path ./.prettierignore"
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		],
		"rules": {
			"no-restricted-globals": [
				"error",
				"event",
				"fdescribe"
			]
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@fluentui/react": "^8.71.0",
		"@fluidframework/azure-client": "^2.0.0-internal.7.3.0",
		"@fluidframework/routerlicious-driver": "^2.0.0-internal.7.3.0",
		"@fluidframework/test-runtime-utils": "^2.0.0-internal.7.3.0",
		"axios": "^1.3.6",
		"cross-env": "^7.0.3",
		"fluid-framework": "^2.0.0-internal.7.3.0",
		"react": "^16.10.2",
		"react-dnd": "^14.0.2",
		"react-dnd-html5-backend": "^14.0.0",
		"react-dom": "^16.10.2",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@fluidframework/azure-local-service": "^2.0.0-internal.7.3.0",
		"@fluidframework/build-common": "^2.0.2",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/node": "^18.0.0",
		"@types/react": "^16.9.15",
		"@types/react-dom": "^16.9.4",
		"@types/uuid": "^8.3.0",
		"jest": "^29.7.0",
		"jest-junit": "^16.0.0",
		"jest-puppeteer": "^9.0.0",
		"prettier": "^2.7.1",
		"puppeteer": "^21.0.0",
		"react-scripts": "^5.0.1",
		"start-server-and-test": "^2.0.0",
		"typescript": "~4.5.5"
	},
	"jest-junit": {
		"outputDirectory": "nyc",
		"outputName": "jest-junit-report.xml"
	}
}
