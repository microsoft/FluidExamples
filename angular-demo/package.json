{
	"name": "@fluid-example/angular-demo",
	"private": true,
	"version": "0.1.0",
	"homepage": "https://fluidframework.com",
	"repository": "microsoft/FluidExamples",
	"license": "MIT",
	"author": "Microsoft and contributors",
	"scripts": {
		"ci:test": "start-server-and-test start:server 7070 ci:test:jest",
		"ci:test:jest": "jest --ci --reporters=default --reporters=jest-junit",
		"ng": "ng",
		"start": "ng serve",
		"start:server": "npx tinylicious",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "start-server-and-test start:server 7070 test:jest",
		"test:jest": "jest",
		"format": "npm run prettier:fix",
		"lint": "npm run prettier",
		"lint:fix": "npm run prettier:fix",
		"prettier": "prettier --check . --ignore-path ./.prettierignore",
		"prettier:fix": "prettier --write . --ignore-path ./.prettierignore"
	},
	"dependencies": {
		"@angular/animations": "~13.3.11",
		"@angular/common": "~13.3.11",
		"@angular/compiler": "~13.3.11",
		"@angular/core": "~13.3.11",
		"@angular/forms": "~13.3.11",
		"@angular/platform-browser": "~13.3.11",
		"@angular/platform-browser-dynamic": "~13.3.11",
		"@angular/router": "~13.3.11",
		"@fluidframework/tinylicious-client": "^1.3.0",
		"fluid-framework": "^1.0.1",
		"tslib": "^2.3.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~13.3.9",
		"@angular/cli": "~13.3.9",
		"@angular/compiler-cli": "~13.3.11",
		"@fluidframework/build-common": "^1.1.0",
		"@types/debug": "^4.1.7",
		"@types/node": "^14.18.0",
		"jest": "^29.3.0",
		"jest-junit": "^10.0.0",
		"jest-puppeteer": "^6.1.1",
		"prettier": "^2.7.1",
		"puppeteer": "^19.2.2",
		"start-server-and-test": "^1.14.0",
		"tinylicious": "^0.4.94771",
		"typescript": "~4.5.5"
	},
	"jest-junit": {
		"outputDirectory": "nyc",
		"outputName": "jest-junit-report.xml"
	}
}
