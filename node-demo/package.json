{
	"name": "@fluid-example/node-demo",
	"version": "0.1.0",
	"repository": "microsoft/FluidExamples",
	"main": "./src/index.js",
	"scripts": {
		"build": "npm build",
		"start:client": "node ./src/index.js",
		"start:server": "npx tinylicious@latest",
		"test": "mocha --timeout 20000 ./test/*.test.js",
		"test:report": "start-server-and-test start:server 7070 test",
		"format": "npm run prettier:fix",
		"lint": "npm run prettier",
		"lint:fix": "npm run prettier:fix",
		"prettier": "prettier --check . --ignore-path ./.prettierignore",
		"prettier:fix": "prettier --write . --ignore-path ./.prettierignore"
	},
	"license": "MIT",
	"dependencies": {
		"@fluidframework/tinylicious-client": "^2.0.0-internal.4.1.0",
		"fluid-framework": "^2.0.0-internal.4.0.5",
		"readline-sync": "^1.4.10",
		"prettier": "^2.7.1"
	},
	"devDependencies": {
		"@fluidframework/build-common": "^2.0.0-151899",
		"chai": "^4.3.7",
		"mocha": "^10.1.0",
		"sinon": "^14.0.2",
		"sinon-chai": "^3.7.0",
		"start-server-and-test": "^1.14.0"
	},
	"type": "module"
}
